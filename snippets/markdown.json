{
    "Generate toc": {
        "prefix": "toc",
        "body": [
            "<md-toc></md-toc>"
        ]
    },
    "Generate cover": {
        "prefix": "cover",
        "body": [
            "<md-cover title='${1:title}' author='${2:author}'></md-cover>$0"
        ]
    },
    "Generate page": {
        "prefix": "page",
        "body": [
            "<md-page></md-page>"
        ]
    },
    "Generate image": {
        "prefix": "img",
        "body": [
            "<md-img src='$1' alt='$2'></md-img>"
        ]
    },
    "Generate todo": {
        "prefix": "-",
        "body": [
            "- [ ] "
        ]
    },
    "Generate bibliography": {
        "prefix": "bib",
        "description": "Create bibliography",
        "body": [
            "<md-bib src='${1:source}'></md-bib>$0"
        ]
    },
    "Generate bib url": {
        "prefix": "bib-url",
        "body": [
            "<md-bib-url accessed=\"$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE\"->$1</md-bib-url>$0"
        ]
    },
    "Generate bib from doi": {
        "prefix": "bib-doi",
        "body": [
            "<md-bib-doi>$1</md-bib-doi>$0"
        ]
    },
    "Collage": {
        "prefix": "collage",
        "body": [
            "<ul collage>\n\t$TM_SELECTED_TEXT\n</ul>$0"
        ]
    },
    "Add Style": {
        "prefix": "style",
        "body": [
            "<md-style name=\"${1|latex,acm,github,dropbox,latex report,somnox,tudelft|}\"></md-style>$0"
        ],
        "description": ""
    }
}