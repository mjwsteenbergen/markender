{
	"name": "markender",
	"displayName": "markender",
	"description": "Creating beautiful markdown documents",
	"version": "2.0.1",
	"publisher": "mjwsteenbergen",
	"repository": {
		"type": "git",
		"url": "https://github.com/mjwsteenbergen/markender"
	},
	"engines": {
		"vscode": "^1.40.0"
	},
	"keywords": [
		"markdown",
		"tools",
		"web-components"
	],	
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [],
		"markdown.previewScripts": [
			"components/dist/dist.all/main.bundle.js"
		],
		"markdown.previewStyles": [
			"components/morecss.css"
		],
		"snippets": [
			{
				"language": "markdown",
				"path": "./snippets/markdown.json"
			}
		],
		"markdown.markdownItPlugins": true
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"docs": "node ./doc.js"
	},
	"dependencies": {
		"markdown-it-enml-todo": "^1.0.2",
		"markdown-it-katex": "^2.0.3"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "^15.0.2",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^8.3.2",
		"typescript": "^4.2.4",
		"tslint": "^6.1.3",
		"vscode-test": "^1.5.2"
	}
}